NODE_ENV=production
NEXT_TELEMETRY_DISABLED=1
ATP_CLOUD_ACCESS_KEY=atp-internal-dev-key-2024
SECURITY_AUDIT_ENABLED=true
RATE_LIMITING_ENABLED=true
RBAC_ENABLED=true

# Better Auth Configuration
BETTER_AUTH_SECRET=your-secure-random-secret-here-64-chars-minimum
BETTER_AUTH_URL=https://agenttrustprotocol.com
NEXT_PUBLIC_BASE_URL=https://agenttrustprotocol.com

# OAuth Providers (Get from Google/GitHub developer consoles)
GOOGLE_CLIENT_ID=YOUR_REAL_GOOGLE_CLIENT_ID_HERE
GOOGLE_CLIENT_SECRET=YOUR_REAL_GOOGLE_CLIENT_SECRET_HERE
GITHUB_CLIENT_ID=YOUR_REAL_GITHUB_CLIENT_ID_HERE
GITHUB_CLIENT_SECRET=YOUR_REAL_GITHUB_CLIENT_SECRET_HERE

# Email Service (Resend)
EMAIL_PROVIDER=resend
RESEND_API_KEY=your-resend-api-key
EMAIL_FROM=noreply@agenttrustprotocol.com
EMAIL_FROM_NAME=Agent Trust Protocol

# Database (if using external DB)
# DATABASE_URL=your-database-url-here
